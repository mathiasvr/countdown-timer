{
  "name": "menubar-timer",
  "productName": "Menubar Timer",
  "version": "0.0.0",
  "description": "Menubar Countdown Timer App",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . --extra-resource=static/Credits.rtf --icon=static/Icon.icns --platform=darwin --arch=x64 --out=build --tmpdir=false",
    "makeicon": "iconutil --convert icns static/Icon.iconset",
    "test": "standard"
  },
  "keywords": [
    "timer",
    "countdown",
    "stopwatch",
    "menubar",
    "electron"
  ],
  "author": "Mathias Rasmussen <mathiasvr@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mathiasvr/countdown-timer.git"
  },
  "bugs": {
    "url": "https://github.com/mathiasvr/countdown-timer/issues"
  },
  "homepage": "https://github.com/mathiasvr/countdown-timer#readme",
  "dependencies": {
    "blob-to-buffer": "^1.2.6",
    "hhmmss": "^1.0.0",
    "menubar": "^5.1.0",
    "tiny-timer": "^1.0.2",
    "yo-yo": "^1.3.0"
  },
  "devDependencies": {
    "electron": "1.4.2",
    "electron-packager": "^8.1.0"
  }
}
